programa
{
	
	funcao inicio()
	{
		inteiro num[101]

		para(inteiro i=0; i < 101; i++){
			 se(i%2 == 0) {
			 	num[i] = i
			 	escreva("\n", num[i])
			 }
		}
	}
}
